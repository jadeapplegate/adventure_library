<h1>Jade's Choose Your Own Adventure</h1>

<% @adventures.each do |adventure| %>
  	<% if adventure.pages.present? %>
			<%=  link_to adventure.title, adventure_page_path(adventure, adventure.pages.first.id) %> <br />
		<% end %>
	<% end %>

<%= link_to "Create a New Adventure", new_adventure_path %>